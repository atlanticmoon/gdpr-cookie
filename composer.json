{
    "name": "atlanticmoon/gdpr-cookie",
    "description": "Laravel cookie checker for GDPR Compliance",
    "keywords": [
        "atlanticmoon",
        "gdpr-cookie",
        "gdpr",
        "cookie"
    ],
    "homepage": "https://github.com/atlanticmoon/gdpr-cookie",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlanticmoon Srl (developed by Fabrizio Zippo)",
            "email": "zippo@atlanticmoon.it",
            "homepage": "https://www.atlanticmoon.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/routing": "5.5.x|5.6.x|5.7.x",
        "illuminate/support": "5.5.x|5.6.x|5.7.x",
        "illuminate/view": "5.5.x|5.6.x|5.7.x",
        "illuminate/cookie": "5.5.x|5.6.x|5.7.x"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Atlanticmoon\\GdprCookie\\": "src"
        }
    },
    "autoload-dev": {
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atlanticmoon\\GdprCookie\\GdprCookieServiceProvider"
            ],
            "aliases": {
                "GdprCookie": "Atlanticmoon\\GdprCookie\\GdprCookieFacade"
            }
        }
    }
}
